{
  "expo": {
    "name": "GTM - Validateur",
    "slug": "gtm-validateur",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/gtm.png",
    "scheme": "gtmvalidateur",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/gtm.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.achaumba.gtmvalidateur"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/gtm.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/gtm.png",
          "imageWidth": 200,
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "83fa6b10-db97-455c-803c-bfbb5501877c"
      }
    }
  }
}
